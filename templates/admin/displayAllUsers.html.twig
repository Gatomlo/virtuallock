      modal.find('.modal-body').text('Voulez-vous vraiment supprimer l\'utilisateur : "'+user+'".');
      modal.find('#deleteButton').attr('href', url);
    })
    $('#promoteUserModal').on('show.bs.modal', function (event) {
      var button = $(event.relatedTarget); // Button that triggered the modal
      user = button.data('name');
      url = button.data('url');
      var modal = $(this);
      modal.find('.modal-body').text('Voulez-vous vraiment faire de cet utilisater un administrateur ? Cette action est définitive et irréversible.');
      modal.find('#promoteButton').attr('href', url);
    })
    $('#linkModal').on('show.bs.modal', function (event) {
      var button = $(event.relatedTarget); // Button that triggered the modal
      cadenas = button.data('name');
      url = button.data('url');
      var modal = $(this);
      modal.find('.modal-title').text(cadenas);
      modal.find('#url').val(url);
      modal.find('#iframe').val('<iframe src="'+url+'" height="500" width="350" frameborder="0" ></iframe>')
      // new QRCode(document.getElementById("qrcode"), url);
      var qrcode = new QRCode(document.getElementById("qrcode"), {
      	text: url,
      	width: 256,
      	height: 256,
      	colorDark : "#000000",
      	colorLight : "#ffffff",
      	correctLevel : QRCode.CorrectLevel.H
      });
    })
    $('#linkModal').on('shown.bs.modal',function(){
      var imgUrl = $('#qrcode').find('img').attr('src');
      $('#dllButton').attr('href',imgUrl);
      $('#dllButton').attr('download', cadenas);
    });
    $('#linkModal').on('hidden.bs.modal',function(event){
      $('#qrcode').children().remove();
    })
    $('#soluceModal').on('shown.bs.modal',function(event){
      var button = $(event.relatedTarget)
      cadenas = button.data('name');
      type = button.data('type');
      solution = button.data('soluce').toString();
      $('#soluceZone').html(solution);
      var modal = $(this);
      modal.find('.modal-title').text(cadenas);
      var arraySolution = solution.split(',');
      displaySoluceInform(type,arraySolution);
      modal.find('button[id="listen"]').click(function() {
        arraySolution.forEach((item, i) => {
        setTimeout(function () {
          createSound(item);
          $('.piano-'+item).addClass("blueKey")
          setTimeout(function () {
              $('.blueKey').removeClass("blueKey");
          }, 200);
        }, 500 * i);
        });
        function createSound(key){
          var obj = document.createElement("audio");
          obj.src = "{{ asset('samples/') }}"+key+".ogg";
          obj.autoPlay = false;
          obj.preload = true;
          obj.controls = false;
          obj.play();
        }
   });
   if (type == "diagram"){
     var schema = arraySolution.join("");
     diagram= new PatternLock('#soluceZone',{
      lineOnMove:false,
      enableSetPattern : true,
      onDraw:function(data){}
      });
     diagram.setPattern(schema);
     diagram.disable();
   }
  });
